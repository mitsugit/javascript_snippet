<script>
        async function callApi(){
        const res = await fetch("https://jsonplaceholder.typicode.com/users");
        //console.log(res);
        const users = await res.json();
        console.log(users);
        name1 = users[0]["name"];
        name2 = users[1]["name"];
        name3 = users[2]["name"];
        document.getElementById('gaga1').value = name1;
        document.getElementById('gaga2').value = name2;
        document.getElementById('gaga3').value = name3;
        }


        async function callApi2(){
        const res = await fetch("https://jsonplaceholder.typicode.com/users");
        //console.log(res);
        const users = await res.json();
        console.log(users);
        name4 = users[3]["name"];
        name5 = users[4]["name"];
        name6 = users[5]["name"];
        document.getElementById('gaga4').value = name4;
        document.getElementById('gaga5').value = name5;
        document.getElementById('gaga6').value = name6;
        }


</script>

